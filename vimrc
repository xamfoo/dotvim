set backspace=indent,eol,start
set belloff=all
set hidden
set incsearch
set mouse=a
set nobackup
set noswapfile
set nowritebackup
set nrformats-=octal
set number
set ruler
set shortmess+=c
set updatetime=300
let g:mapleader="\t"
syntax on
ino <F1> <Esc>
nno <F1> <Esc>
nno / /\c
ino jk <Esc>
cno jk <Esc>
